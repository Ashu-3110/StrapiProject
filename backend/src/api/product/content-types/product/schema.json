{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Flavor": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "blocks"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-detail.product-detail",
      "mappedBy": "product"
    },
    "product_size_cost": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-size-cost.product-size-cost",
      "mappedBy": "product"
    }
  }
}
